<article>
  <% @creatures.each do |creature| %>
    <div class="">
      <a href="<%= creature_path(creature) %>"><img src="<%= creature.thumb.main %>" alt="" /></a>
      <p><span style="color: red;"><%= creature.category %></span> | <%= creature.title %></p>
    </div>
  <% end %>
</article>

